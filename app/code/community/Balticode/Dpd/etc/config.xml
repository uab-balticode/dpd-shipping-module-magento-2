<?xml version="1.0" encoding="UTF-8"?>
<config>
    <modules>
        <Balticode_Dpd>
            <version>0.2.0</version>
        </Balticode_Dpd>
    </modules>
    <global>
        <blocks>
            <dpd>
                <class>Balticode_Dpd_Block</class>
            </dpd>
            <adminhtml>
                <rewrite>
                    <sales_order_create_shipping_method_form>Balticode_Dpd_Block_Adminhtml_Sales_Order_Create_Shipping_Method_Form</sales_order_create_shipping_method_form>
                </rewrite>
            </adminhtml>
        </blocks>
        <helpers>
            <dpd>
                <class>Balticode_Dpd_Helper</class>
            </dpd>
        </helpers>
        <models>
            <dpd>
                <class>Balticode_Dpd_Model</class>
                <resourceModel>dpd_resource</resourceModel>
            </dpd>
            <dpd_resource>
                <class>Balticode_Dpd_Model_Resource</class>
                <entities>
                    <deliverypoints>
                        <table>balticode_dpd_delivery_point</table>
                    </deliverypoints>
                    <deliveryprice>
                        <table>balticode_dpd_delivery_price</table>
                    </deliveryprice>
                </entities>
            </dpd_resource>
            <sales>
                <rewrite>
                    <order>Balticode_Dpd_Model_Sales_Order</order>
                </rewrite>
            </sales>
        </models>
        <resources>
            <dpd_setup>
                <setup>
                    <module>Balticode_Dpd</module>
                </setup>
            </dpd_setup>
            <connection>
                <use>core_setup</use>
            </connection>
        </resources>
        <events>
            <admin_system_config_changed_section_carriers>
                <observers>
                    <dpd>
                        <type>singleton</type>
                        <class>dpd/observer</class>
                        <method>changedSettings</method>
                    </dpd>
                </observers>
            </admin_system_config_changed_section_carriers>
            <sales_quote_save_after>
                <observers>
                    <dpd>
                        <type>singleton</type>
                        <class>dpd/observer</class>
                        <method>quoteSaveAfter</method>
                    </dpd>
                </observers>
            </sales_quote_save_after>
            <sales_order_place_after>
                <observers>
                    <dpd>
                        <class>dpd/observer</class>
                        <method>newOrderPlaceAfter</method>
                    </dpd>
                </observers>
            </sales_order_place_after>
            <sales_order_view_shipping_information_after>
                <observers>
                    <dpd>
                        <class>dpd/observer</class>
                        <method>saleOrderViewShippingInformation</method>
                    </dpd>
                </observers>
            </sales_order_view_shipping_information_after>
            <core_block_abstract_prepare_layout_before>
                <observers>
                    <dpd>
                        <type>singleton</type>
                        <class>dpd/observer</class>
                        <method>addBulkAction</method>
                    </dpd>
                </observers>
            </core_block_abstract_prepare_layout_before>
        </events>
    </global>
    <admin>
         <routers>
             <balticode_dpd>
                 <use>admin</use>
                 <args>
                     <module>Balticode_Dpd</module>
                     <frontName>dpd</frontName>
                 </args>
             </balticode_dpd>
         </routers>
    </admin>
    <frontend>
        <routers>
            <balticode_dpd>
                <use>standard</use>
                <args>
                    <module>Balticode_Dpd</module>
                    <frontName>dpd</frontName>
                </args>
            </balticode_dpd>
        </routers>
        <layout>
            <updates>
                <dpd>
                    <file>dpd.xml</file>
                </dpd>
            </updates>
        </layout>
        <translate>
            <modules>
                <dpd>
                    <files>
                        <default>Balticode_Dpd.csv</default>
                    </files>
                </dpd>
            </modules>
        </translate>
    </frontend>
    <adminhtml>
        <layout>
            <updates>
                <dpd>
                    <file>dpd.xml</file>
                </dpd>
            </updates>
        </layout>
        <translate>
            <modules>
                <Balticode_Dpd>
                    <files>
                        <Balticode_Dpd>Balticode_Dpd.csv</Balticode_Dpd>
                    </files>
                </Balticode_Dpd>
            </modules>
        </translate>
        <events>
            <controller_action_predispatch>
                <observers>
                    <dpd>
                        <type>singleton</type>
                        <class>dpd/observer</class>
                        <method>preDispatch</method>
                    </dpd>
                </observers>
            </controller_action_predispatch>
        </events>
    </adminhtml>
